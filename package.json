{
  "name": "es-mapping-ts",
  "version": "0.0.15",
  "description": "ES Mapping TypeScript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "test": "node_modules/.bin/jest --colors --no-cache --config=./jest.it.config.js --runInBand --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "peerDependencies": {
    "bluebird": "^3.5.1",
    "elasticsearch": "^14.2.2",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.105",
    "@types/node": "^10.7.1",
    "bluebird": "^3.5.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.1.3",
    "elasticsearch": "^14.2.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-typescript": "^3.2.3",
    "jest": "^21.1.8",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "tslint": "5.3.2",
    "typescript": "^2.6.2",
    "typescript-babel-jest": "^1.0.5"
  },
  "keywords": [
    "typescript",
    "elasticsearch",
    "mapping",
    "documentation",
    "decorator"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xrobert35/es-mapping-ts.git"
  },
  "author": "xrobert",
  "license": "MIT"
}
